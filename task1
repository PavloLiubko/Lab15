#include <iostream> 

using namespace std;

int main() {

	setlocale(LC_ALL, "UKR");

	int n;
	cout << "Введіть кількість елементів масиву А:";
	cin >> n;
	
	int A[n];
	for (int i = 0; i < n; i++) {
		cout << endl << "Введіть якийсь значення для елементу масиву:";
		cin >> A[i];
	}

	int counter = 0;

	for (int i = 0; i < n; i++) {
		if (A[i] == 0) {
			counter += 1;
		}
	}

	cout << endl << "Кількість нульових елементів у нашому масиві: " << counter;
	

	return 0;
} 
